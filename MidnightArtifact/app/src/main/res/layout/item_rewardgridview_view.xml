<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.base.baselibs.widget.RoundImageView
        android:id="@+id/item_rewardlist_head_img"
        android:layout_width="@dimen/dimen_dp_30"
        android:layout_height="@dimen/dimen_dp_30"
        android:src="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/item_rewardlist_more_img"
        android:layout_width="@dimen/dimen_dp_30"
        android:layout_height="@dimen/dimen_dp_30"
        android:src="@mipmap/reward_more"/>

</RelativeLayout>